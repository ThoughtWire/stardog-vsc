{
    "name": "sparql",
    "displayName": "language-sparql",
    "description": "SPARQL grammar",
    "version": "0.0.1",
    "publisher": "stardog-union",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "sparql",
            "aliases": ["SPARQL", "sparql"],
            "extensions": [".rq",".sparql"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "sparql",
            "scopeName": "source.sparql",
            "path": "./syntaxes/sparql.tmLanguage"
        }]
    }
}